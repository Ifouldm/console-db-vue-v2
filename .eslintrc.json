{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "extends": ["plugin:vue/essential", "airbnb-base", "prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "parser": "babel-eslint",
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": ["vue"],
    "rules": {
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off"
    }
}
